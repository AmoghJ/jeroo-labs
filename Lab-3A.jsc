@Java
method spiralIn()
{
   while (!isFlower(AHEAD))
   {
      if (isWater(AHEAD))
         turn(RIGHT);
      if (!isFlower(AHEAD))
         hop();
   }
}

method spiralOut()
{
   while (!(isWater(AHEAD) && isWater(RIGHT) && isNet(LEFT)))
   {
      if (isFlower(AHEAD))
         turn(RIGHT);
      else if (isNet(AHEAD) || isWater(AHEAD))
         turn(LEFT);
      hop();
   }
}
